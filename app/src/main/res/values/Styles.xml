<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="tituloOpenSans">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/open_sans_bold</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="texto_grande_blanco">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:fontFamily">@font/open_sans_semibold</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">50sp</item>
    </style>
    <style name="texto_blanco_16">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:fontFamily">@font/open_sans_bold</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="texto_blanco_13">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/open_sans_semibold</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="texto_verde_20">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/open_sans_bold</item>
        <item name="android:textColor">@color/verde_fit</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="texto_verde_13">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/open_sans_light</item>
        <item name="android:textColor">@color/verde_fit</item>
        <item name="android:textSize">13dp</item>
    </style>
    <style name="boton_verde">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/background_dialog</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="boton_gris">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/boton_gris</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="boton_rutina_verde">
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:background">@drawable/background_dialog</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="texto_opacidad_media">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/open_sans_light</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">13sp</item>
        <item name="android:translationX">15dp</item>
        <item name="android:translationY">15dp</item>
    </style>
    <style name="barra_navegacion">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/background_color</item>
    </style>

    <style name="skip_previous">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/gris</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:fontFamily">@font/open_sans_bold</item>
        <item name="android:textSize">17dp</item>
    </style>




    <style name="radio_button">
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="radio_button_gris">
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:background">@drawable/unchecked_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">10sp</item>
    </style>





    <style name="texto_titulo_blanco_20" parent="CustomFont">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:layout_marginEnd">160dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="checkbox_blanco_8">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">8sp</item>
        <item name="android:buttonTint">@color/white</item>
        <item name="fontFamily">@font/open_sans_semibold</item>
    </style>
    <style name="texto_blanco_10">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">80dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/open_sans</item>
    </style>
    <style name="texto_sans_18_blanco">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:fontFamily">@font/open_sans</item>
